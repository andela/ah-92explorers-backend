{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run migrate && npm run seed\" \"babel-node index.js && redis-server\"",
    "dev": "NODE_ENV=development nodemon ./index.js --exec babel-node",
    "test": "NODE_ENV=test npm run setup:test && NODE_ENV=test mocha --require @babel/register ./tests --timeout 10000 --exit",
    "setup:test": "NODE_ENV=test npm run migrate:undo && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed",
    "setup:dev": "NODE_ENV=development npm run migrate:undo && NODE_ENV=development npm run migrate && NODE_ENV=development npm run seed",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls && nyc --reporter=lcov --reporter=text-lcov npm test",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "lint": "eslint . --ext .js",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@sendgrid/mail": "^6.4.0",
    "algoliasearch": "^3.33.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "concurrently": "^4.1.1",
    "connect-session-sequelize": "~6.0.0",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.2",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "he": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mailgen": "^2.0.7",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemailer": "^6.2.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.19.1",
    "open": "^6.4.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-plus": "^0.5.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "random-string": "^0.2.0",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "sequelize": "^5.9.4",
    "sequelize-cli": "^5.5.0",
    "sequelize-slugify": "^0.7.0",
    "socket.io": "*",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^3.0.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "nyc": {
    "exclude": [
      "tests/*",
      "models/*",
      "helpers/",
      "migrations/*",
      "middlewares/checkDb.js",
      "seeders/*",
      "config/*",
      "index.js"
    ]
  }
}
